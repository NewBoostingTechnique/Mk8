{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet: build",
      "program": "${workspaceFolder}/src/Web/bin/Debug/net8.0/Mk8.Web.dll",
      "cwd": "${workspaceFolder}/src/Web",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "MySql.Console",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet: build",
      "program": "${workspaceFolder}/src/MySql.Console/bin/Debug/net8.0/Mk8.MySql.Console.dll",
      "cwd": "${workspaceFolder}/src/MySql.Console",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
